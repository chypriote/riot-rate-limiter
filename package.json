{
  "name": "riot-rate-limiter",
  "version": "0.2.0",
  "description": "A rate limiter handling rate-limits enforced by the riot-games api",
  "keywords": [
    "TypeScript",
    "nodeJs",
    "riot-api",
    "ratelimiter"
  ],
  "contributors": [{
    "name": "Jonas Krispin",
    "url": "Colorfulstan/RiotRateLimiter-node"
  }, {
    "name": "Chypriote",
    "url": "chypriote/riot-rate-limiter"
  }],
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf build coverage",
    "clean:dist": "rimraf dist",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "compile:build": "tsc",
    "compile:dist": "tsc -p tsconfig.dist.json",
    "compile:watch": "tsc -w",
    "test-with-cover": "nyc mocha build/**/*test.js",
    "test": "mocha build/**/*test.js",
    "coverage": "nyc check-coverage ./coverage/typescript/coverage.json",
    "build": "npm-run-batch",
    "build:dist": "run-batch",
    "build:watch": "cross-env NODE_ENV=production run-batch",
    "precommit": "run-batch",
    "prepush": "run-batch"
  },
  "run-batch": {
    "build": ["clean", "lint", "compile:build", "test"],
    "build:watch": ["compile:watch"],
    "build:dist": ["lint", "clean", "compile:build", "test", "clean:dist", "compile:dist"],
    "precommit": ["build"],
    "prepush": ["build:dist"]
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/chai-as-promised": "7.1.5",
    "@types/circular-json": "^0.4.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.3.3",
    "@types/semver": "^7.5.0",
    "bottleneck": "^2.19.5",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "graceful-fs": "^4.2.11",
    "husky": "^8.0.3",
    "mocha": "^10.2.0",
    "node-emoji": "^2.1.0",
    "npm-run-batch": "^0.0.9",
    "nyc": "^15.1.0",
    "parse-git-config": "^3.0.0",
    "rimraf": "^5.0.1",
    "shelljs-nodecli": "^0.1.1",
    "sinon": "^15.2.0",
    "ts-node": "^10.9.1",
    "tslint": "^5.20.1",
    "typescript": "^5.1.6",
    "semver": "^7.5.3"
  },
  "dependencies": {
    "request-promise": "^4.2.1",
    "runtime-engine-check": "^1.0.0"
  },
  "standard": {
    "env": [
      "mocha"
    ]
  }
}
